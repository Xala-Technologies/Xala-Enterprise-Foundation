{
  "name": "@xala-technologies/{{PACKAGE_NAME}}",
  "version": "1.0.0",
  "description": "{{PACKAGE_DESCRIPTION}}",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --passWithNoTests",
    "test:compliance": "jest --testNamePattern='compliance|norwegian|gdpr'",
    "test:accessibility": "jest --testNamePattern='accessibility|a11y|wcag'",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test:ci",
    "compliance:quick": "node scripts/norwegian-compliance-check.js",
    "compliance:full": "node scripts/full-compliance-validation.js",
    "security:audit": "npm audit --audit-level=moderate",
    "validate": "npm run lint && npm run typecheck && npm run test:ci && npm run compliance:quick"
  },
  "keywords": [
    "xala",
    "norwegian",
    "government",
    "compliance",
    "{{NSM_CLASSIFICATION}}",
    "gdpr",
    "accessibility",
    "enterprise",
    "{{PACKAGE_NAME}}",
    "{{PACKAGE_DISPLAY_NAME}}"
  ],
  "author": {
    "name": "Xala Technologies",
    "email": "support@xalatechnologies.com",
    "url": "https://xalatechnologies.com"
  },
  "license": "UNLICENSED",
  "homepage": "https://github.com/{{GITHUB_ORG}}/{{PACKAGE_NAME}}#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/{{GITHUB_ORG}}/{{PACKAGE_NAME}}.git"
  },
  "bugs": {
    "url": "https://github.com/{{GITHUB_ORG}}/{{PACKAGE_NAME}}/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@xala-technologies/foundation": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.19.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "xala": {
    "packageType": "{{PACKAGE_NAME}}",
    "displayName": "{{PACKAGE_DISPLAY_NAME}}",
    "nsmClassification": "{{NSM_CLASSIFICATION}}",
    "norwegianCompliance": true,
    "gdprCompliant": true,
    "accessibilityCompliant": true,
    "platforms": [
      "web",
      "mobile",
      "desktop",
      "api"
    ],
    "languages": [
      "nb",
      "nn",
      "en"
    ]
  }
}
